                    <% }) %>
            </section>
        </div>
        <div class="col-sm-4">
          <section id="displaysection" >
                    <% arr_filenames.forEach(function(item){ %>
                    <h5> <%= item.filename %> </h5>     
                    <% }) %>
            </section>
        </div>
        <div class="col-sm-3">
          <section id="displaysection" >
                    <% arr_receivernames.forEach(function(item){ %>
                    <h5> <%= item.firstname %> </h5> 
                    <% }) %>
            </section>
        </div>
    </div>


